    "@types/react-table": "^6.8.7",
    "@types/reactstrap": "^8.4.2",
    "@types/sha.js": "^2.4.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
