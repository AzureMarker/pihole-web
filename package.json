{
  "name": "pi-hole_web",
  "version": "1.0.0",
  "private": true,
  "homepage": "/admin",
  "proxy": "http://pi.hole",
  "devDependencies": {
    "faker": "^4.1.0",
    "fs-extra": "^6.0.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "node-fetch": "^2.1.2",
    "multi-progress": "^2.0.0",
    "react-scripts": "^1.1.4"
  },
  "dependencies": {
    "@coreui/coreui": "^2.0.2",
    "@coreui/coreui-plugin-chartjs-custom-tooltips": "^1.2.0",
    "@coreui/icons": "0.2.0",
    "@coreui/react": "^2.0.4",
    "bootstrap": "^4.1.1",
    "chart.js": "^2.7.2",
    "i18next": "^11.3.2",
    "i18next-browser-languagedetector": "^2.2.0",
    "i18next-xhr-backend": "^1.5.1",
    "ionicons": "^3.0.0",
    "font-awesome": "^4.7.0",
    "simple-line-icons": "^2.4.1",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-chartjs-2": "^2.7.2",
    "react-dom": "^16.4.1",
    "react-i18next": "^7.6.1",
    "react-router-dom": "^4.3.1",
    "react-table": "^6.8.2",
    "react-transition-group": "^2.3.1",
    "reactstrap": "^6.1.0",
    "sha.js": "^2.4.11"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "start-fake": "node make-fake-data.js && REACT_APP_FAKE_API=1 npm run start",
    "build": "npm-run-all build-css build-js",
    "build-fake": "node make-fake-data.js && REACT_APP_FAKE_API=1 npm run build",
    "build-js": "react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/scss",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules ./node_modules/simple-line-icons/scss ./node_modules/font-awesome/scss ./src/scss -o ./src/scss --watch --recursive",
    "update-translations": "node update-translations.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
